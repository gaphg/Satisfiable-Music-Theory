DECLARE VOICES: v1, v2
DECLARE LENGTH: 8

INCLUDE: ../example_rules/western_stdlib.rules

REQUIRE: pitches-of v1 at 0t = 60p
REQUIRE: contour-of v1 at 0t = M2 up
REQUIRE: contour-of v1 at 1t = M2 up
REQUIRE: contour-of v1 at 2t = m2 up
REQUIRE: contour-of v1 at 3t = M2 up
REQUIRE: contour-of v1 at 4t = 2i up
REQUIRE: contour-of v1 at 5t = 2i up
REQUIRE: contour-of v1 at 6t = 1i up
REQUIRE: contour-of v1 at 7t = 2i up

REQUIRE: diads-of(v1, v2) at t in [ 3i up , 4i up ]
REQUIRE: flatten(pitches-of v2 at t) in [ 0p, 2p, 4p, 5p, 7p, 9p, 11p ]